{
    "manifest_version": 3,
    "name": "8ookmarks",
    "description": "test",
    "version": "0.1",
    "icons": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    },
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_icon": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
      }
    },
    "permissions": [
        "scripting",
        "activeTab",
        "sidePanel",
        "tabs",
        "contextMenus",
        "storage"
    ],
    "commands": {
      "_execute_action": {
        "suggested_key": {
          "default": "Ctrl+B",
          "mac": "Command+B"
        }
      }
    },
    "side_panel": {
        "default_path": "sidepanel.html"
    }
  }